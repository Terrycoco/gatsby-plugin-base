{
  "name": "gatsby-plugin-base",
  "version": "1.0.0",
  "description": "Basis for starting a gatsby plugin",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir ./ --source-maps",
    "watch": "babel src --out-dir ./ --source-maps --watch",
    "test": "jest --color",
    "lint": "eslint --ext js ./ --cache",
    "format": "prettier --write 'src/*.js'"
  },
  "author": "Coco",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "jest": "^29.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-babel": "^5.3.1",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0",
    "reading-time": "^1.5.0"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0"
  },
  "jest": {
    "collectCoverage": true
  },
  "publishConfig": {
    "access": "public"
  }
}
